@article{sen_maritime_2014,
  title = {Maritime {{Southeast Asia Between South Asia}} and {{China}} to the {{Sixteenth Century}}},
  author = {Sen, Tansen},
  date = {2014-01},
  journaltitle = {TRaNS: Trans-Regional and -National Studies of Southeast Asia},
  shortjournal = {TRaNS},
  volume = {2},
