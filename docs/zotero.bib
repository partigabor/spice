@book{gracie_florapedia_2021,
  title = {Florapedia: {{A Brief Compendium}} of {{Floral Lore}}},
  author = {Gracie, Carol},
  date = {2021},
  publisher = {{Princeton University Press}},
  location = {{Princeton}},
